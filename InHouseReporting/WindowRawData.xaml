<Window x:Class="InHouseReporting.WindowRawData"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Raw Data" Height="286" Width="516" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid Height="249">
        <DataGrid ItemsSource="{Binding}" AutoGenerateColumns="True" Height="200" HorizontalAlignment="Left" Margin="8,9,0,0" Name="dataGridResult" VerticalAlignment="Top" Width="474" />
        <Button Content="Delete" Height="23" HorizontalAlignment="Left" Margin="326,215,0,0" Name="buttonDelete" VerticalAlignment="Top" Width="75" Click="buttonDelete_Click" />
        <Button Content="Close" Height="23" HorizontalAlignment="Right" Margin="0,215,12,0" Name="buttonClose" VerticalAlignment="Top" Width="75" Click="buttonClose_Click" />
    </Grid>
</Window>
